{
	"name": "sveltekit-starter-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . --ext .js,.ts,.svelte",
		"lint:fix": "prettier --check . && eslint --fix . --ext .js,.ts,.svelte",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "husky",
		"dev:android": "pnpm run dev --host 0.0.0.0 && cap sync android && cap open android",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@capacitor/cli": "^6.0.0",
		"@fontsource/poppins": "^5.0.14",
		"@playwright/test": "^1.44.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.3",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"autoprefixer": "^10.4.19",
		"cssnano": "^7.0.1",
		"cssnano-preset-advanced": "^7.0.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.5",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "5.0.0-next.143",
		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.12",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"engines": {
		"npm": "please-use-pnpm",
		"yarn": "please-use-pnpm",
		"pnpm": ">=9.0.0",
		"node": ">=18.13.0"
	},
	"dependencies": {
		"@capacitor/android": "^6.0.0",
		"@capacitor/core": "^6.0.0",
		"@capacitor/ios": "^6.0.0"
	}
}
